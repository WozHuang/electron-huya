{
  "name": "electron-huya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-react\" \"npm run dev-electron\" ",
    "dev-react": "react-app-rewired start",
    "dev-electron": "node script/dev-runner.js",
    "build": "npm run build-react && npm run build-electron && npm run build-packager",
    "build-react": "react-app-rewired build",
    "build-electron": "webpack --config config/webpack.main.config.js",
    "build-packager": "electron-packager build --out=release --overwrite --asar --icon=public/favicon.png"
  },
  "homepage": ".",
  "dependencies": {
    "antd": "^3.17.0",
    "axios": "^0.21.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "video.js": "^7.5.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ]
  },
  "browserslist": [
    "last 2 chrome version"
  ],
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "customize-cra": "^0.2.12",
    "electron": "^5.0.1",
    "electron-debug": "^3.0.0",
    "electron-packager": "^13.1.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.0.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
